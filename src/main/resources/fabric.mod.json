{
  "schemaVersion": 1,
  "id": "altioptimized",
  "version": "${version}",
  "name": "altiOptimized",
  "description": "A Minecraft optimization mod that automatically configures video settings to their lowest values for maximum FPS. Includes AI-powered optimization commands.",
  "authors": [
    "GamerAlti"
  ],
  "contact": {
    "homepage": "https://altioptimized.netlify.app/",
    "sources": "https://github.com/Ayaan192/Altioptimized"
  },
  "license": "CC0-1.0",
  "icon": "assets/altioptimized/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "com.altioptimized.Altioptimized"
    ],
    "client": [
      "com.altioptimized.client.AltioptimizedClient"
    ]
  },
  "mixins": [
    "altioptimized.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}